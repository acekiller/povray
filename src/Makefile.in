	@echo "Generating jversion.h"
	strings /usr/lib/libjpeg.a /usr/local/lib/libjpeg.a | grep '^.*  [0-3]*[0-9]-[A-Z][a-z][a-z]-[12][0-9][0-9][0-9]' | head -n 1 | sed 's/^\(.*\)/#define JVERSION \"\1\"/' > jversion.h || echo "ERROR: Can't find libjpeg.a"

