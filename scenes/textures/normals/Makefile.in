install-normalsDATA: $(normals_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(normalsdir)
	@list='$(normals_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(normalsdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(normalsdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(normalsdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(normalsdir)/$$p; \
	  fi; fi; \
	done

uninstall-normalsDATA:
	@$(NORMAL_UNINSTALL)
	list='$(normals_DATA)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(normalsdir)/$$p; \
	done
